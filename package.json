  "description": "transactions processor",
  "repository": {
    "type": "git",
    "url": "https://github.com/crowdanalyzer/transactions.git"
  },
  "engine": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "contributors": [
    {
      "name": "Mohamed Ashour <mohamed-ashour>",
